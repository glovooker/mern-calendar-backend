{
    "version": 2,
    "builds": [
        {
            "src": "index.js", 
            "use": "@vercel/node"
        },
        {
            "src": "/public/.*\\.js",
            "headers": {
                "Content-Type": "application/javascript"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
