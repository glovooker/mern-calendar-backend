{
    "version": 2,
    "builds": [
        {
            "src": "index.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "headers": [
        {
          "source": "/public/(.*\\.js)",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/javascript"
            }
          ]
        }
      ]
}
